  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.38"
  },
  "build": {
    "appId": "com.app.climatewallet",
    "productName": "Climate Wallet",
    "asarUnpack": [
      "node_modules/node-mac-permissions",
      "node_modules/keytar"
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      "extraResources/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "climate-wallet",
      "owner": "chia"
    },
    "mac": {
      "icon": "src/assets/img/climate_wallet-green.png",
      "target": [
        {
          "target": "dmg"
        }
      ]
    },
    "win": {
      "icon": "src/assets/img/climate_wallet-green.png",
      "target": [
        {
          "target": "nsis"
        }
      ]
    },
    "linux": {
      "icon": "src/assets/img/climate_wallet-green.png",
      "target": [
        {
          "target": "deb"
        }
      ]
    }
  }
